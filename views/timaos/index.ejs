<h1>Lista de timaos</h1>
<a href="/timao/new" class="btn btn-success">Novo Timao</a>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Timao</th>
        </tr>
    </thead>
    <tbody>
        <% timaos.forEach(timao=> { %>
            <tr>
                <td>
                    <%= timao.id %>
                </td>
                <td>
                    <%= timao.nome %>
                </td>
                <td>
                    <a href="/timao/<%= timao.id %>" class="btn btn-primary">View</a>
                    <a href="/timao/<%= timao.id %>/edit" class="btn btn-warning">Edit</a>
                    <form action="/timao/<%= timao.id %>?_method=DELETE" method="POST"
                        style="display: inline;">
                        <button type="submit" class="btn btn-danger">Delete</button>
                    </form>
                </td>
            </tr>
            <% }) %>
    </tbody>
</table>